{
  "name": "rest-api-setup",
  "version": "1.0.0",
  "description": "A REST API setup using Node.js, Express, MySQL, TypeORM, and TypeScript.",
  "main": "src/app.ts",
  "scripts": {
    "start": "npx rimraf dist && npm run build && tsc && node dist/app.js",
    "build": "tsc",
    "dev": "ts-node-dev src/app.ts",
    "restart": "npx rimraf dist && npm run build"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "express-auto-routes": "^2.2.0",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.0.0",
    "typeorm-model-generator": "^0.4.6",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
